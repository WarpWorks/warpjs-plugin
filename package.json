{
  "name": "@warp-works/warpjs-plugin",
  "version": "1.2.3",
  "description": "WarpJS base plugin",
  "main": "index.js",
  "scripts": {
    "prepack": "npm run test",
    "precoverage": "npm run lint",
    "coverage": "npm run -s coverage:unit",
    "coverage:unit": "nyc --report-dir=reports/coverage/unit npm run -s test:unit",
    "lint": "npm run -s lint:node && npm run -s lint:test",
    "lint:node": "eslint --fix --config=.eslintrc.node.json --ignore-pattern \"*.test.js\" *.js lib",
    "lint:test": "eslint --fix --config=.eslintrc.test.json \"lib/**/*.test.js\" \"*.test.js\"",
    "pretest": "npm run lint",
    "test": "npm run -s test:unit",
    "test:unit": "mocha \"*.unit.test.js\" \"lib/**/*.unit.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WarpWorks/warpjs-plugin.git"
  },
  "keywords": [
    "warp-works",
    "warpjs",
    "plugin"
  ],
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WarpWorks/warpjs-plugin/issues"
  },
  "homepage": "https://github.com/WarpWorks/warpjs-plugin#readme",
  "dependencies": {
    "lodash": "~4.17.10"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "latest",
    "@quoin/node-test-helpers": "1.x.x",
    "eslint": "~5.16.0",
    "eslint-plugin-html": "~5.0.3",
    "eslint-plugin-import": "2.x.x",
    "eslint-plugin-json": "1.x.x",
    "eslint-plugin-node": "~8.0.1",
    "eslint-plugin-promise": "~4.1.1",
    "eslint-plugin-standard": "~4.0.0",
    "mocha": "~6.0.2",
    "nyc": "~13.3.0"
  }
}
